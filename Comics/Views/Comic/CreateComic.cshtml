@{
    ViewBag.title = "Crear Comic";
}
<h2> Crear Comic</h2>
<div ng-controller="ComicsCreateController">
    <form novalidate class="simple-form">
        <label>Nombre: <input type="text" ng-model="comic.titulo" /></label><br />
        <label>Fundada: <input type="text" ng-model="comic.anio" /></label>
        <input type="button" ng-click="reset()" value="Reset" />
        <input type="submit" ng-click="CrearComic(comic)" value="Save" />
    </form>
</div>
