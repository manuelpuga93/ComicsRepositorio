@{
    ViewBag.title = "Crear Compa&ntilde;ia";
}
<h2> Crear Compa&ntilde;ia</h2>
<div ng-controller="CompaniaCreateController">
    <form novalidate class="simple-form">
        <div class="form-group"><label>Nombre: <input class="form-control" type="text" ng-model="compania.nombre" /></label></div>
        <div class="form-group"><label>Fundada: <input class="form-control" type="text" ng-model="compania.founded" /></label></div>
        <input  class="btn btn-primary" type="submit" ng-click="CrearCompania(compania)" value="Guardar" />
    </form>
</div>